{
  "name": "altv-boilerplate-js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch ./modules -e js -x \"node ./scripts/build.js && cd altv-server && altv-server.exe\"",
    "win": "node ./scripts/build.js -m && cd altv-server && altv-server.exe",
    "linux": "node ./scripts/build.js -m && cd altv-server && ./altv-server",
    "update": "node ./scripts/install.js"
  },
  "dependencies": {
    "fs-extra": "^11.1.1",
    "nodemon": "^2.0.22",
    "terser": "^5.17.1"
  }
}
